{
  "name": "@fjell/lib-sequelize",
  "version": "4.4.81",
  "license": "Apache-2.0",
  "keywords": [
    "library",
    "persistence",
    "database",
    "orm",
    "sequelize",
    "fjell"
  ],
  "description": "Sequelize Library for Fjell",
  "engines": {
    "node": ">=21"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run lint && node build.js",
    "dev": "node build.js --watch",
    "lint": "eslint . --ext .ts --fix",
    "clean": "rm -rf dist",
    "test": "vitest run --coverage",
    "precommit": "npm run clean && npm run lint && npm run build && npm run test",
    "prepublishOnly": "npm run clean && npm run build",
    "docs:dev": "cd docs && npm run dev",
    "docs:build": "cd docs && npm run build",
    "docs:preview": "cd docs && npm run preview",
    "docs:test": "cd docs && npm run test"
  },
  "dependencies": {
    "@fjell/core": "^4.4.73",
    "@fjell/lib": "^4.4.82",
    "@fjell/logging": "^4.4.65",
    "@fjell/registry": "^4.4.81",
    "dayjs": "^1.11.13",
    "deepmerge": "^4.3.1",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.32.0",
    "@fjell/common-config": "^1.1.36",
    "@swc/core": "^1.13.2",
    "@tsconfig/recommended": "^1.0.10",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "esbuild": "0.25.9",
    "eslint": "^9.32.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getfjell/lib-sequelize.git"
  }
}
